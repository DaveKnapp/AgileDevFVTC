@model IEnumerable<T5.Brothership.Entities.Models.User>

@{
    ViewBag.Title = "Users Who Play This Game";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users Who Play This Game</h2>

<table class="table">
    <tr>
        <th>
            Search Results
            @*@Html.DisplayNameFor(model => model.UserName)*@
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.ProfileImagePath)*@
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.UserName, "User", "User", new { userName = item.UserName }, null)
            </td>
            <td>
                <img src="@item.ProfileImagePath" alt="">
            </td>

        </tr>
    }

</table>
